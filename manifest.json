{
  "name": "Galaxy Scraper Extension",
  "description": "Select links on a page and send data directly to your Galaxy history.",
  "version": "0.1",
  "homepage_url": "https://github.com/fabio-cumbo/galaxy-scraper-extension",
  "minimum_chrome_version": "16.0.884",
  "permissions": ["<all_urls>"],
  "background": {
    "persistent": true,
    "scripts": ["js/jquery-3.2.1.min.js", "background.js"]
  },
  "browser_action": {"default_popup": "popup.html"},
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon-16.png",
    "19": "icons/icon-19.png",
    "38": "icons/icon-38.png",
    "64": "icons/icon-64.png",
    "128": "icons/icon-128.png"
  }
}
